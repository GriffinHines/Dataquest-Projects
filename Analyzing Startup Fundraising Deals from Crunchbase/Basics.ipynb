      "total percentage of funds raised by top 10% of companies: 67%\n",
      "total percentage of funds raised by top 1% of companies: 26%\n",
      "total percentage of funds raised by bottom 10% of companies: 0.037%\n",
      "total percentage of funds raised by bottom 1% of companies: 0.00026%\n"
     ]
    }
   ],
   "source": [
    "query = '''SELECT SUM(raised_amount_usd) as total_raised FROM investments'''\n",
    "total_investments = pd.read_sql(query, conn).iloc[0]['total_raised']\n",
    "\n",
    "#top 10 percent\n",
    "query = '''SELECT company_name, SUM(raised_amount_usd) as total_raised FROM investments\n",
    "GROUP BY company_name\n",
    "ORDER BY total_raised desc\n",
    "LIMIT 1157'''\n",
    "top_ten_percent_dataframe = pd.read_sql(query, conn)\n",
    "top_ten_percent = top_ten_percent_dataframe['total_raised'].sum()\n",
    "print(\"total percentage of funds raised by top 10% of companies:\", str(int(round(top_ten_percent/total_investments, 2)*100)) + '%')\n",
    "\n",
    "#top 1 percent\n",
    "query = '''SELECT company_name, SUM(raised_amount_usd) as total_raised FROM investments\n",
    "GROUP BY company_name\n",
    "ORDER BY total_raised desc\n",
    "LIMIT 116'''\n",
    "top_one_percent_dataframe = pd.read_sql(query, conn)\n",
    "top_one_percent = top_one_percent_dataframe['total_raised'].sum()\n",
    "print(\"total percentage of funds raised by top 1% of companies:\", str(int(round(top_one_percent/total_investments, 2)*100)) + '%')\n",
    "\n",
    "#bottom 10 percent\n",
    "query = '''SELECT company_name, SUM(raised_amount_usd) as total_raised FROM investments\n",
    "GROUP BY company_name\n",
    "ORDER BY total_raised asc\n",
    "'''\n",
    "bottom_ten_percent = pd.read_sql(query, conn).dropna().head(1157)['total_raised'].sum()\n",
    "print(\"total percentage of funds raised by bottom 10% of companies:\", str(round(bottom_ten_percent/total_investments*100, 3)) + '%')\n",
    "\n",
    "#bottom 1 percent\n",
    "bottom_one_percent = pd.read_sql(query, conn).dropna().head(116)['total_raised'].sum()\n",
    "print(\"total percentage of funds raised by bottom 1% of companies:\", str(round(bottom_one_percent/total_investments*100, 5)) + '%')"
   ]
  },
